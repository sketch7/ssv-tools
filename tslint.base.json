{
	"rules": {
		"align": [
			"arguments",
			"parameters",
			"statements",
			true
		],
		"array-type": [
			"array-simple",
			true
		],
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"arrow-return-shorthand": [
			"multiline",
			true
		],
		"class-name": true,
		"comment-format": [
			"check-space",
			true
		],
		"curly": true,
		"eofline": false,
		"forin": true,
		"import-spacing": true,
		"indent": [
			"tabs",
			true
		],
		"interface-over-type-literal": true,
		"label-position": true,
		"max-line-length": [
			140,
			true
		],
		"member-access": false,
		"member-ordering": [
			"public-before-private",
			"static-before-instance",
			true,
			"variables-before-functions"
		],
		"no-angle-bracket-type-assertion": true,
		"no-arg": true,
		"no-bitwise": true,
		// "no-boolean-literal-compare": false,
		"no-consecutive-blank-lines": [
			true,
			2
		],
		"no-console": [
			"debug",
			"info",
			"time",
			"timeEnd",
			"trace",
			true
		],
		"no-construct": true,
		"no-debugger": true,
		"no-duplicate-variable": true,
		"no-empty": true,
		"no-eval": true,
		"no-inferrable-types": [
			true
		],
		"no-invalid-this": true,
		"no-magic-numbers": false,
		"no-shadowed-variable": true,
		"no-string-literal": true,
		"no-string-throw": true,
		"no-switch-case-fall-through": true,
		"no-trailing-whitespace": true,
		"no-unnecessary-initializer": true,
		"no-unused-expression": true,
		"no-unused-variable": [
			{
				"ignore-pattern": "^_"
			},
			"check-parameters",
			true
		],
		"no-use-before-declare": true,
		"no-var-keyword": true,
		"object-literal-key-quotes": [
			"as-needed",
			true
		],
		"object-literal-shorthand": true,
		"object-literal-sort-keys": false,
		"one-line": [
			"check-catch",
			"check-else",
			"check-open-brace",
			"check-whitespace",
			true
		],
		"only-arrow-functions": [
			"allow-declarations",
			"allow-named-functions",
			true
		],
		"prefer-const": true,
		"prefer-for-of": true,
		"prefer-method-signature": false,
		"quotemark": [
			"double",
			true
		],
		"radix": true,
		"semicolon": [
			true,
			"always"
		],
		"space-before-function-paren": [
			true,
			"never"
		],
		"trailing-comma": [
			{
				"multiline": "never",
				"singleline": "never"
			},
			false
		],
		"triple-equals": [
			"allow-null-check",
			true
		],
		"typedef-whitespace": [
			{
				"call-signature": "nospace",
				"index-signature": "nospace",
				"parameter": "nospace",
				"property-declaration": "nospace",
				"variable-declaration": "nospace"
			},
			true
		],
		"use-isnan": true,
		"variable-name": false,
		"whitespace": [
			"check-branch",
			"check-decl",
			"check-operator",
			"check-separator",
			"check-type",
			"check-typecast",
			true
		]
	}
}